<%- include("navbar") %>

<h2>History</h2>
<table>
    <tr>
        <th>Action</th>
        <th>Data</th>
        <th>Timestamp</th>
    </tr>
    <% history.forEach(entry => { %>
    <tr>
        <td><%= entry.action %></td>
        <td><%= JSON.stringify(entry.data) %></td>
        <td><%= entry.timestamp.toLocaleString() %></td>
    </tr>
    <% }); %>
</table>

